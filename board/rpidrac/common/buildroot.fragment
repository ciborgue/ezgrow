BR2_SYSTEM_ENABLE_NLS=y
BR2_TARGET_TZ_INFO=y
BR2_ENABLE_LOCALE_PURGE=n

# enable X Window system
BR2_PACKAGE_XORG7=y
BR2_PACKAGE_XSERVER_XORG_SERVER=y

# web browser is required
BR2_PACKAGE_DILLO=y
BR2_PACKAGE_OPENSSL=y

# Java VM
BR2_PACKAGE_JAMVM=y

# RatPoison is display manager
BR2_PACKAGE_RATPOISON=y
